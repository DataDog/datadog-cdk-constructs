  "types": "lib/index.d.ts",
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "datadog-cdk-constructs-v2",
        "module": "datadog_cdk_constructs_v2"
