{
    "provider": "hashicorp/azurerm",
    "resource": "azurerm_windows_web_app",
    "fields": {
        "impl": [
            "app_settings"
        ],
        "always_allow": [
            "key_vault_reference_identity_id",
            "zip_deploy_file"
        ],
        "never_allow": [
            "java_container",
            "java_container_version",
            "python",
            "tomcat_version",
            "docker_image_name",
            "docker_registry_password",
            "docker_registry_url",
            "docker_registry_username"
        ],
        "always_sensitive": [
            "app_settings",
            "site_config"
        ]
    },
    "additional_providers": [
        {
            "name": "Azure/azapi",
            "version": "2.5.0"
        }
    ]
}
